# [alias]
#     co = checkout
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
    interactive = auto
    pager = true
[core]
    excludesfile = ~/.gitignore_global
    pager = less -FX
    editor = code --wait
[credential]
    helper = cache --timeout=3600
[help]
	autocorrect = prompt
# [push]
#     default = simple
# [branch "master"]
#     mergeoptions = --no-edit
[pull]
    rebase = false
[init]
    defaultBranch = main
